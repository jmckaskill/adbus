<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioToolFile
	Name="ctct_tonka Rules"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="UIC"
			DisplayName="Qt UI"
			CommandLine="uic.exe [inputs] -o &quot;$(IntDir)\ui_$(InputName).h&quot;"
			Outputs="$(IntDir)\ui_$(InputName).h"
			FileExtensions="*.ui"
			ExecutionDescription="$(InputName).ui"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="QRC"
			DisplayName="Qt Resource"
			CommandLine="rcc.exe [inputs] -o $(IntDir)\$(InputName)_qrc.cpp &amp;&amp; cl.exe $(IntDir)\$(InputName)_qrc.cpp [AdditionalIncludeDirectories] [PreprocessorDefinitions] [CompilerAdditionalOptions] /nologo /c /Fo&quot;$(IntDir)\$(InputName)_qrc.obj&quot;"
			Outputs="$(IntDir)\$(InputName)_qrc.obj"
			FileExtensions="*.qrc"
			ExecutionDescription="$(InputName).qrc"
			>
			<Properties>
				<StringProperty
					Name="AdditionalIncludeDirectories"
					DisplayName="Additional Include Directories"
					Category="General"
					Switch="-I[value]"
					Delimited="true"
					Inheritable="true"
				/>
				<StringProperty
					Name="PreprocessorDefinitions"
					DisplayName="Preprocessor Definitions"
					Category="General"
					Switch="-D[value]"
					Delimited="true"
					Inheritable="true"
				/>
				<StringProperty
					Name="CompilerAdditionalOptions"
					DisplayName="Compiler Command Line Options"
					Category="General"
					Switch="[value]"
				/>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="MOC"
			DisplayName="Qt Meta Object"
			CommandLine="moc.exe [AdditionalIncludeDirectories] [PreprocessorDefinitions] [MocAdditionalOptions] -D_MSC_VER=1500 -D_WIN32 [Inputs] -o &quot;$(IntDir)\$(InputName)_moc.cpp&quot; &amp;&amp; cl.exe $(IntDir)\$(InputName)_moc.cpp [AdditionalIncludeDirectories] [PreprocessorDefinitions] [CompilerAdditionalOptions] /nologo /c /Fo&quot;$(IntDir)\$(InputName)_moc.obj&quot;"
			Outputs="$(IntDir)\$(InputName)_moc.obj"
			FileExtensions="*.hxx"
			ExecutionDescription="$(InputName).hxx"
			>
			<Properties>
				<StringProperty
					Name="AdditionalIncludeDirectories"
					DisplayName="Additional Include Directories"
					Category="General"
					Switch="-I[value]"
					Delimited="true"
					Inheritable="true"
				/>
				<StringProperty
					Name="PreprocessorDefinitions"
					DisplayName="Preprocessor Definitions"
					Category="General"
					Switch="-D[value]"
					Delimited="true"
					Inheritable="true"
				/>
				<StringProperty
					Name="MocAdditionalOptions"
					DisplayName="Moc Additional Command Line Options"
					Category="General"
					Switch="[value]"
				/>
				<StringProperty
					Name="CompilerAdditionalOptions"
					DisplayName="Compiler Command Line Options"
					Category="General"
					Switch="[value]"
				/>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="QDBUSXML2CPP"
			DisplayName="Qt DBus XML to C++"
			CommandLine="qdbusxml2cpp.exe [Inputs] -p &quot;$(IntDir)\$(InputName)_interface&quot; [InterfaceAdditionalOptions] &amp;&amp; qdbusxml2cpp.exe [Inputs] -a &quot;$(IntDir)\$(InputName)_adaptor&quot; [AdaptorAdditionalOptions] &amp;&amp; moc.exe [AdditionalIncludeDirectories] [PreprocessorDefinitions] -D_MSC_VER=1500 -D_WIN32 &quot;$(IntDir)\$(InputName)_interface.h&quot; -o &quot;$(IntDir)\$(InputName)_interface_moc.cpp&quot; &amp;&amp; moc.exe [AdditionalIncludeDirectories] [PreprocessorDefinitions] -D_MSC_VER=1500 -D_WIN32 &quot;$(IntDir)\$(InputName)_adaptor.h&quot; -o &quot;$(IntDir)\$(InputName)_adaptor_moc.cpp&quot; &amp;&amp; cl.exe &quot;$(IntDir)\$(InputName)_interface.cpp&quot; [AdditionalIncludeDirectories] [PreprocessorDefinitions] [CompilerAdditionalOptions] /nologo /c /Fo&quot;$(IntDir)\$(InputName)_interface.obj&quot; &amp;&amp; cl.exe &quot;$(IntDir)\$(InputName)_adaptor.cpp&quot; [AdditionalIncludeDirectories] [PreprocessorDefinitions] [CompilerAdditionalOptions] /nologo /c /Fo&quot;$(IntDir)\$(InputName)_adaptor.obj&quot; &amp;&amp; cl.exe &quot;$(IntDir)\$(InputName)_interface_moc.cpp&quot; [AdditionalIncludeDirectories] [PreprocessorDefinitions] [CompilerAdditionalOptions] /nologo /c /Fo&quot;$(IntDir)\$(InputName)_interface_moc.obj&quot; &amp;&amp; cl.exe &quot;$(IntDir)\$(InputName)_adaptor_moc.cpp&quot; [AdditionalIncludeDirectories] [PreprocessorDefinitions] [CompilerAdditionalOptions] /nologo /c /Fo&quot;$(IntDir)\$(InputName)_adaptor_moc.obj&quot;"
			Outputs="$(IntDir)\$(InputName)_adaptor.h;;$(IntDir)\$(InputName)_interface.h;$(IntDir)\$(InputName)_adaptor.obj;;$(IntDir)\$(InputName)_interface.obj;;$(IntDir)\$(InputName)_adaptor_moc.obj;$(IntDir)\$(InputName)_interface_moc.obj"
			FileExtensions="*.dbus"
			ExecutionDescription="$(InputName).dbus"
			>
			<Properties>
				<StringProperty
					Name="AdditionalIncludeDirectories"
					DisplayName="Additional Include Directories"
					Category="General"
					Switch="-I[value]"
					Delimited="true"
					Inheritable="true"
				/>
				<StringProperty
					Name="PreprocessorDefinitions"
					DisplayName="Preprocessor Definitions"
					Category="General"
					Switch="-D[value]"
					Delimited="true"
					Inheritable="true"
				/>
        <StringProperty
          Name="AdaptorAdditionalOptions"
          DisplayName="Adaptor Additional Options"
          Category="General"
          Switch="[value]"
        />
        <StringProperty
          Name="InterfaceAdditionalOptions"
          DisplayName="Interface Additional Options"
          Category="General"
          Switch="[value]"
        />
				<StringProperty
					Name="CompilerAdditionalOptions"
					DisplayName="Compiler Command Line Options"
					Category="General"
					Switch="[value]"
				/>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
