include_rules

CFLAGS += $(CFLAGS_QT_CORE) $(CFLAGS_QT_NETWORK) $(CFLAGS_QT_XML) $(CFLAGS_QT_DBUS)
CFLAGS += -DQDBUS_MAKEDLL
CFLAGS += -fvisibility=hidden

MOCFLAGS_qdbuspendingcall.hxx += -fqdbuspendingcall_p.hxx
MOCFLAGS_qdbusservicewatcher.hxx += -fqdbusservicewatcher_p.h

: foreach *.hxx |> !moc |> moc_%B.cpp
: foreach *.cpp |> !cpp |> %B.o

